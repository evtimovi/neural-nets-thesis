Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"meb", "[", "r_", "]"}], ":=", 
    RowBox[{"r", "[", 
     RowBox[{"[", 
      RowBox[{"4", ";;", "259"}], "]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "used", " ", "to", " ", "get", " ", "just", " ", "the", " ", "MEB", " ", 
     "from", " ", "the", " ", "row"}], ";", " ", 
    RowBox[{
    "the", " ", "first", " ", "3", " ", "entries", " ", "are", " ", 
     "metadata", " ", "such", " ", "as", " ", "subjid", " ", "and", " ", 
     "imgcode"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"quantize", "[", "x_", "]"}], ":=", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"x", ">", "0.5"}], ",", "1", ",", "0"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"quantize", " ", "a", " ", "single", " ", "number"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"quantizedMEB", "[", "r_", "]"}], ":=", 
    RowBox[{"quantize", "/@", 
     RowBox[{"meb", "[", "r", "]"}]}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "return", " ", "the", " ", "quantized", " ", "MEB", " ", "for", " ", "the",
     " ", "row"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"targetMEBForSubject", "[", 
     RowBox[{"subjid_", ",", "data_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"meb", "/@", 
       RowBox[{"Select", "[", 
        RowBox[{"data", ",", 
         RowBox[{
          RowBox[{"isTargetForSubject", "[", 
           RowBox[{"subjid", ",", "#"}], "]"}], "&"}]}], "]"}]}], ")"}], "[", 
     
     RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "  ", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "find", " ", "the", " ", "real", " ", "MEB", " ", "of", " ", "the", " ", 
     "given", " ", "subject"}], ";", " ", 
    RowBox[{"returns", " ", "just", " ", "a", " ", "single", " ", "list"}]}], 
   " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"allNetworkMEBsForSubject", "[", 
     RowBox[{"subjid_", ",", "data_"}], "]"}], ":=", 
    RowBox[{"quantizedMEB", "/@", 
     RowBox[{"Select", "[", 
      RowBox[{"data", ",", 
       RowBox[{
        RowBox[{"isSubjectAndNotTarget", "[", 
         RowBox[{"subjid", ",", "#"}], "]"}], "&"}]}], "]"}]}]}], ";"}], " ", 
  
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "find", " ", "all", " ", "the", " ", "MEBs", " ", "for", " ", "the", " ", 
     "given", " ", "subject"}], ";", " ", 
    RowBox[{"returns", " ", "a", " ", "list", " ", "of", " ", "lists"}]}], 
   " ", "*)"}]}]}], "Input"],

